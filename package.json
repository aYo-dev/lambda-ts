{
  "name": "lambda-ts",
  "version": "1.1.2",
  "description": "typescript implementation of algebraic data types and functional programming patterns",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "start": "./node_modules/.bin/ts-node src/index.ts",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aYo-dev/functional-typescript.git"
  },
  "keywords": [
    "functional programming",
    "typescript",
    "category theory"
  ],
  "author": "Asen Yovchev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aYo-dev/functional-typescript/issues"
  },
  "homepage": "https://github.com/aYo-dev/functional-typescript#readme",
  "dependencies": {
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
